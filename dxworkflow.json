    {
      "id": "stage-rpt_athena",
      "executable": "app-eggd_athena/1.6.0",
      "input": {
        "panel_bed": {
          "$dnanexus_link": {
            "outputField": "bed_file",
            "stage": "stage-rpt_generate_bed_athena"
          }
        }
